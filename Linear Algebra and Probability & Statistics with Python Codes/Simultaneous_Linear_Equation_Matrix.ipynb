   "source": [
    "import numpy as np\n",
    "\n",
    "#행렬을 활용해 연립일차방정식 풀기\n",
    "coef = np.array([[1,2], [2, 3]])\n",
    "cons = np.array([[0], [0]])\n",
    "\n",
    "value = np.linalg.solve(coef, cons)\n",
    "print(value)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "id": "d6ee4c14",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
